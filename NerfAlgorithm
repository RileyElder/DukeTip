import numpy as np
x=[-40,-20,0,20,40,60,80]
y=[61,177.33,296.33,680.33,651,534.67,24.67,]
result = np.polyfit(x,y,3)
eq = np.poly1d(result)
x2=np.arange(-40,90)
yfit = np.polyval(result,x2)
try:
    user_input=int(raw_input('Input angle between -40 and 90'))
    if user_input>=int(-40):
        if user_input<=int(90):
            print eq(user_input)
    else:
        print 'Please stay within assigned range'
